<script>
  $('#nav_menu_admin_service').addClass('active')
</script>
<div class="container-fluid main-container">

  <%= render(:partial => "/sidebar/adservice") %>
  <div class="main-content clearfix">
    <div class="breadcrumbs" id="breadcrumbs">
      <ul class="breadcrumb">
        <li>
          <i class="icon-home"></i>
          能力开发

			<span class="divider">
				<i class="icon-angle-right"></i>
			</span>
        </li>
        <li class="active">能力报表</li>
      </ul>
      <!--.breadcrumb-->
    </div>
    <div id="content" class="page-content">
      <div class="page-header position-relative">
        <h1>
          能力报表
          <small>
            <i class="icon-double-angle-right"></i>
            <span>接入号报表</span>
          </small>
        </h1>
      </div>

      <div class="box-content">
        <ul class="nav nav-tabs" id="myTab">
          <li class="<%="active" if (params[:msgtype]==nil||params[:msgtype]=="day")%>"><a a data-toggle="tab" href="#info">接入号日度报表 </a></li>
          <li class="<%="active" if (params[:msgtype]=="month")%>"><a a data-toggle="tab" href="#custom">接入号月度报表 </a></li>
          <li class="<%="active" if (params[:msgtype]=="year")%>"><a a data-toggle="tab"  href="#messages">接入号年度报表 </a></li>
        </ul>

        <div id="myTabContent" class="tab-content">
          <div class="<%="tab-pane "%><%="active" if (params[:msgtype]==nil||params[:msgtype]=="day")%>" id="info">
            <%= render :partial=>"adservice/acctabday",:locals=>{:year=>@showday,:type=>"day"}%>
          </div>
          <div class="<%="tab-pane "%><%="active" if (params[:msgtype]=="month")%>" id="custom">
            <%= render :partial=>"adservice/acctabmonth",:locals=>{:year=>@showmonth,:type=>"month"}%>
          </div>
          <div class="<%="tab-pane "%><%="active" if (params[:msgtype]=="year")%>" id="messages">

            <%= render :partial=>"adservice/acctabyear",:locals=>{:year=>@showyear,:type=>"year"}%>
          </div>
        </div>




    </div>
  </div>
</div>
</div>



